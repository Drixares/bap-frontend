{
  "trailingComma": "es5",
  "tabWidth": 4,
  "semi": true,
  "singleQuote": false,
  "printWidth": 80,
  "jsxBracketSameLine": false,
  "arrowParens": "always",
  "insertPragma": false,
  "proseWrap": "preserve",
  "importOrder": [
    "^[./]",
    "<alphabetical>"
  ],
  "importOrderSeparation": true,
  "importOrderCaseInsensitive": true,
  "overrides": [
    {
      "files": "*.tsx",
      "options": {
        "parser": "typescript",
        "printWidth": 100
      }
    }
  ]
}
